module count_args;

import std.system;
import std.io;

func main() -> int {
    let count: int = argc();
    printl_si("No. of arguments: ", count);
    for (let i = 0; i < count; i = i + 1 ) {
        printl_is(i, argv(i));
    }
    return 0;
}