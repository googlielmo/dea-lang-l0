module fact;

import std.io;

func printsil(s: string, i: int) -> void {
    print_s(s);
    printl_i(i);
}

func factorial(n: int) -> int {
    if (n <= 1) {
        return 1;
    } else {
        return n * factorial(n - 1);
    }
}

func main() -> void {
    let fact5: int = factorial(5);
    printsil("Factorial of 5 is: ", fact5);
}
